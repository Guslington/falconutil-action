{
  "packages": {
    ".": {
      "changelog-path": "CHANGELOG.md",
      "release-type": "simple",
      "bump-minor-pre-major": false,
      "bump-patch-for-minor-pre-major": false,
      "draft": false,
      "prerelease": false,
      "extra-files": [
        {
          "type": "generic",
          "path": "README.md"
        },
        {
          "type": "generic",
          "path": "examples/*.yml"
        }
      ],
      "pull-request-header": ":rocket: New release incoming!",
      "changelog-sections": [
        {
          "type": "feat",
          "section": "Features",
          "hidden": false
        },
        {
          "type": "fix",
          "section": "Bug Fixes",
          "hidden": false
        },
        {
          "type": "chore",
          "section": "Miscellaneous",
          "hidden": false
        },
        {
          "type": "docs",
          "section": "Documentation",
          "hidden": false
        }
      ]
    }
  },
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
